<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.example.plugins">
	<namespace>nec</namespace>
    <entity name="Parts" local-service="true">
        <column name="id" type="long" primary="true"></column>
        <column name="part_number" type="String"></column>
        <column name="description" type="String"></column>
        <column name="item_type" type="String"></column>
        <column name="status" type="String"></column>
        <column name="status_description" type="String"></column>
        <column name="legacy_part_number" type="String"></column>
        <column name="alternate_item" type="String"></column>
        <column name="unit_cost_material_cost" type="double"></column>
        <column name="royalty_cost" type="double"></column>
        <column name="product_code" type="String"></column>
        <column name="product_code_description" type="String"></column>
        <column name="product_group" type="String"></column>
        <column name="product_group_description" type="String"></column>
        <column name="discount_group" type="String"></column>
        <column name="discount_group_description" type="String"></column>
        <column name="us_price_msrp" type="double"></column>
    </entity> 
	
</service-builder>